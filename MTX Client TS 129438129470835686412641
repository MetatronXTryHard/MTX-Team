-- Fast Attack Mode
F:AddDropdown({
    Name = "Fast Attack Mode",
    Description = "",
    Options = {"0", "0.1", "0.5", "0.15"},
    Default = "0.5",
    Flag = "MainFarm/FastattackMode",
    Callback = function(value)
        getgenv().FastAttackDelay = value
    end
  })
--------------------------------------------------------------------------------------------------------------------------------------------
  spawn(function()
    while wait(.1) do
        if  getgenv().FastAttackDelay then
            pcall(function()
                if  getgenv().FastAttackDelay == "0" then
                    getgenv().FastAttackDelay = 0
                elseif  getgenv().FastAttackDelay == "0.1" then
                    getgenv().FastAttackDelay = 0.1
                elseif  getgenv().FastAttackDelay == "0.5" then
                    getgenv().FastAttackDelay = 0.5
                elseif  getgenv().FastAttackDelay == "0.15" then
                    getgenv().FastAttackDelay = 0.15
                end
            end)
        end
    end
end)
