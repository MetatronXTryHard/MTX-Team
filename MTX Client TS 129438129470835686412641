-- Outhers 
local Section = F:AddSection({"Outhers Farm | Options"})

local Toggle = F:AddToggle({"Auto Pirates Castle Raid", Flag = "MainFarm/AutoPiratesCastleRaid", false, function(Value)
    getgenv().RaidPirate = Value
    StopTween(getgenv().RaidPirate)
end})
spawn(function()
    while wait() do
        if getgenv().RaidPirate then
            pcall(function()
                local CFrameBoss = CFrame.new(-5496.17432, 313.768921, -2841.53027, 0.924894512, 7.37058015e-09, 0.380223751, 3.5881019e-08, 1, -1.06665446e-07, -0.380223751, 1.12297109e-07, 0.924894512)
                if (CFrame.new(-5539.3115234375, 313.800537109375, -2972.372314453125).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 500 then
                    for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                        if getgenv().RaidPirate and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                            if (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 2000 then
                                repeat wait()
                                    AutoHaki()
                                    EquipWeapon(getgenv().SelectWeapon)
                                    v.HumanoidRootPart.CanCollide = false
                                    v.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
                                    topos(v.HumanoidRootPart.CFrame * CFrame.new(posX,posY,posZ))
                                    FastClick()
                                until v.Humanoid.Health <= 0 or not v.Parent or not getgenv().RaidPirate
                            end
                        end
                    end
                else
                    UnEquipWeapon(getgenv().SelectWeapon)
                    if BypassTP then
                    if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - CFrameBoss.Position).Magnitude > 1500 then
			        BTP(CFrameBoss)
				    elseif (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - CFrameBoss.Position).Magnitude < 1500 then
				    topos(CFrameBoss)
					end
                    end
                end
            end)
        end
    end
end)

local Toggle = F:AddToggle({"Auto Factory", Flag = "MainFarm/AutoFactory", false, function(Value)
    getgenv().AutoFactory = Value
    StopTween(getgenv().AutoFactory)
end})

spawn(function()
    while wait() do
        pcall(function()
            if getgenv().AutoFactory then
                if game:GetService("Workspace").Enemies:FindFirstChild("Core") then
                    for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                        if v.Name == "Core" and v.Humanoid.Health > 0 then
                            repeat task.wait()
                                AutoHaki()         
                                EquipWeapon(getgenv().SelectWeapon)           
                                topos(CFrame.new(448.46756, 199.356781, -441.389252))                                  
                                FastClick()
                            until v.Humanoid.Health <= 0 or  getgenv().AutoFactory == false
                        end
                    end
                else
                    topos(CFrame.new(448.46756, 199.356781, -441.389252))
                end
            end
        end)
    end
end)

