task.spawn(function()
while true do wait()
    if setscriptable then
        setscriptable(game.Players.LocalPlayer, "SimulationRadius", true)
    end
    if sethiddenproperty then
        sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
    end
end
end)

--[[BringMobNearrest
function InMyNetWork(object)
	if isnetworkowner then
		return isnetworkowner(object)
	else
		if (object.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= getgenv().BringMode then 
			return true
		end
		return false
	end
end

task.spawn(function()
    while task.wait() do
        pcall(function()
            if BringMobNearrest and getgenv().BringMonster then
                for i, v in pairs(game.Workspace.Enemies:GetChildren()) do
                    if not string.find(v.Name, "Boss") and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= getgenv().BringMode then
                        if InMyNetWork(v.HumanoidRootPart) then
                            v.HumanoidRootPart.CFrame = PosGay
                            v.Humanoid.JumpPower = 0
                            v.Humanoid.WalkSpeed = 0
                            v.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
                            v.HumanoidRootPart.Transparency = 1
                            v.HumanoidRootPart.CanCollide = false
                            v.Head.CanCollide = false
                            if v.Humanoid:FindFirstChild("Animator") then
                                v.Humanoid.Animator:Destroy()
                            end
                            v.Humanoid:ChangeState(11)
                            v.Humanoid:ChangeState(14)
                        end
                    end
                end
            end
        end)
    end
end)
]]

task.spawn(function()
  while true do wait()
  if setscriptable then
  setscriptable(game.Players.LocalPlayer,"SimulationRadius",true)
  end
  if sethiddenproperty then
  sethiddenproperty(game.Players.LocalPlayer,"SimulationRadius",math.huge)
  end
  end
  end)

function InMyNetWork(object)
if isnetworkowner then
return isnetworkowner(object)
else
  if (object.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= getgenv().BringMode then
return true
end
return false
end
end

task.spawn(function()
while task.wait() do
    pcall(function()
        if getgenv().AutoSwordMastery and AutoSwordMasteryMag and getgenv().BringMonster then
            for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
                if not string.find(v.Name,"Boss") and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= getgenv().BringMode then
                    if InMyNetWork(v.HumanoidRootPart) then
                        v.HumanoidRootPart.CFrame = PosMon
                        v.Humanoid.JumpPower = 0
                        v.Humanoid.WalkSpeed = 0
                        v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                        v.HumanoidRootPart.Transparency = 1
                        v.HumanoidRootPart.CanCollide = false
                        v.Head.CanCollide = false
                        if v.Humanoid:FindFirstChild("Animator") then
                            v.Humanoid.Animator:Destroy()
                        end
                        v.Humanoid:ChangeState(11)
                        v.Humanoid:ChangeState(14)
                    end
                end
            end
        end
    end)
end
end)

task.spawn(function()
while task.wait() do
    pcall(function()
        if Min_XT_Is_Kak and getgenv().BringMonster then
            for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
                if not string.find(v.Name,"Boss") and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= getgenv().BringMode then
                    if InMyNetWork(v.HumanoidRootPart) then
                        v.HumanoidRootPart.CFrame = PosNarathiwat
                        v.Humanoid.JumpPower = 0
                        v.Humanoid.WalkSpeed = 0
                        v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                        v.HumanoidRootPart.Transparency = 1
                        v.HumanoidRootPart.CanCollide = false
                        v.Head.CanCollide = false
                        if v.Humanoid:FindFirstChild("Animator") then
                            v.Humanoid.Animator:Destroy()
                        end
                        v.Humanoid:ChangeState(11)
                        v.Humanoid:ChangeState(14)
                    end
                end
            end
        end
    end)
end
end)

--------------------------------------------------------------------------------------------------------------------------------------------
-- // Recomended Functions
local Section = S:AddSection({"Recomended | Functions"})

-- Auto Active Race V4
local Toggle = S:AddToggle({"Auto Enable Race V4", Flag = "Settings/EnableRaceV4", false, function(Value)
    getgenv().AutoActiveRaceV4 = Value
end})

spawn(function()
    pcall(function()
        while wait() do
            if getgenv().AutoActiveRaceV4 then
                game:GetService("VirtualInputManager"):SendKeyEvent(true,"Y",false,game)
                wait(0.5)
                game:GetService("VirtualInputManager"):SendKeyEvent(false,"Y",false,game)
            end
        end
    end)
end)
spawn(function()
    while wait() do
		pcall(function()
			if getgenv().AutoActiveRaceV4 then
				game:GetService("VirtualInputManager"):SendKeyEvent(true,"Y",false,game)
				wait(0.5)
                game:GetService("VirtualInputManager"):SendKeyEvent(false,"Y",false,game)
			end
		end)
    end
end)
-- Auto Active Race V3
local Toggle = S:AddToggle({"Auto Enable Race V3", Flag = "Settings/EnableRaceV3",  false, function(Value)
    getgenv().AutoActiveRaceV3 = Value
end})

spawn(function()
    pcall(function()
        while wait() do
            if getgenv().AutoActiveRaceV3 then
                game:GetService("ReplicatedStorage").Remotes.CommE:FireServer("ActivateAbility")
            end
        end
    end)
end)
--------------------------------------------------------------------------------------------------------------------------------------------
-- Lag Remove Functions
local Section = S:AddSection({"Lag Fix | Functions"})
local Toggle = S:AddToggle({"Enable Lag Fix", true, function(Value)
    getgenv().LagFix = Value
end})

spawn(function()
    while wait() do
        if  getgenv().LagFix then
            game:GetService("ReplicatedStorage").Assets.GUI.DamageCounter.Enabled = false
        else
            game:GetService("ReplicatedStorage").Assets.GUI.DamageCounter.Enabled = true
        end
    end
end)

local Toggle = S:AddToggle({"Enable Remove All Notify", Flag = "Settings/RemoveAllNotify", false, function(Value)
    getgenv().RemoveNotify = Value
end})

spawn(function()
    while wait() do
        if getgenv().RemoveNotify then
            game.Players.LocalPlayer.PlayerGui.Notifications.Enabled = false
        else
            game.Players.LocalPlayer.PlayerGui.Notifications.Enabled = true
        end
    end
end)

local Toggle = S:AddToggle({"Enable White Screen", Flag = "Settings/WhiteScreen", false, function(Value)
    getgenv().WhiteScreen = Value
    if getgenv().WhiteScreen == true then
     game:GetService("RunService"):Set3dRenderingEnabled(false)
 elseif getgenv().WhiteScreen == false then
     game:GetService("RunService"):Set3dRenderingEnabled(true)
    end
end})

local Circle = Drawing.new("Circle")
Circle.Color =  Color3.fromRGB(150, 0, 255)
Circle.Thickness = 1
Circle.Radius = 250
Circle.NumSides = 460
Circle.Filled = false
Circle.Transparency = 1

game:GetService("RunService").Stepped:Connect(function()
    Circle.Radius = 20
    Circle.Thickness = 1
    Circle.NumSides = 460
    Circle.Position = game:GetService("UserInputService"):GetMouseLocation()
    if getgenv().WhiteScreen then
        Circle.Visible = true
    else
        Circle.Visible = false
    end
end)

local Button = S:AddButton({"Optimize Game", function(Value)
    getgenv().LagFix2 = Value
end})
spawn(function()
    game:GetService("RunService").RenderStepped:Connect(function()
        if getgenv().LagFix2 == true then
            game:GetService("ReplicatedStorage").Effect.Container.LevelUp:Destroy()
            game:GetService("ReplicatedStorage").Util.Sound:Destroy()
            game:GetService("ReplicatedStorage").Util.Sound.Storage.Other:FindFirstChild("LevelUp_Proxy"):Destroy()
            game:GetService("ReplicatedStorage").Util.Sound.Storage.Other:FindFirstChild("LevelUp"):Destroy()
            game:GetService("ReplicatedStorage").Effect.Container.Respawn:Destroy()        
        end
    end)

end)
