local Section = RD:AddSection("Others | Options")

local Toggle = RD:AddToggle({"Auto Phoenix Raid", false, function(value)
    getgenv().AutoAdvanceDungeon = value
    StopTween(getgenv().AutoAdvanceDungeon)
end})

local Toggle = RD:AddToggle({"Auto Awake", false, function(Value)
    getgenv().AutoAwakenAbilities = Value
end})
spawn(function()
    while task.wait() do
        if getgenv().AutoAwakenAbilities then
